package Student;


public class Student {  
	 String name;  
	 int age;
	 String gender;
	 int id;
	 
}
class TestStudent1{  
	 public static void main(String args[]){  
	  Student s1=new Student(); 
	  s1.name = "vincent";
	  s1.age = 20;
	  s1.gender = "M";
	  s1.id = 2222;
	  System.out.println(s1.name);  
	  System.out.println(s1.age);  
	  System.out.println(s1.gender);
	  System.out.println(s1.id);
	 }  
	}  
