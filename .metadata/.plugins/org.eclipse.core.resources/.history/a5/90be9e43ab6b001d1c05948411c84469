package hallo;

public class hallo {

	public static void main(String args[]) {
	    char ch = '8';
	    int r = 10;
	    switch (ch + 1){
	        case '7' : r = r + 3;break;
	        case '8' : r = r + 5;break;
	        case '9' : r = r + 6;break;
	        default: r = r + 8;
	    }
	    System.out.println(r);
	}

}
